    298            {    
    299              #ifdef ZCL_ON_OFF 
    300                zclGeneral_SendOnOff_CmdToggle( ZIGBEEDEVICE_ENDPOINT, &zclZigbeeDevice_DstAddr, false, 0 );
    301              #endif
    302            }
    303            
    304            //atuo binding(自動綁定裝置)
    305            if ( keys & HAL_KEY_SW2 ) 
    306            {
    307              HalLedSet ( HAL_LED_1, HAL_LED_MODE_OFF );
    308          
