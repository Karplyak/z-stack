    313                                NWK_BROADCAST_SHORTADDR,
    314                                ZCL_HA_PROFILE_ID,
    315                                ZCLZIGBEEDEV_BINDINGLIST, 
    316                                bindingOutClusters,
    317                                0, 
    318                                NULL, // No incoming clusters to bind
    319                                FALSE );
    320            }
    321          }
