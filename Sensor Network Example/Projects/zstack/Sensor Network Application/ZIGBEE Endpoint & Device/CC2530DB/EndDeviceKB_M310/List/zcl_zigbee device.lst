    241          /*********************************************************************
    242           * @fn      zclZigbeeDevice_ProcessZDOMsgs()
    243           * @brief   Process response messages
    244           */
    245          void zclZigbeeDevice_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    246          {
    247            switch ( inMsg->clusterID )
    248            {
    249              case End_Device_Bind_rsp:
    250                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    251                {
    252                  HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    253                }
    254          #if defined(BLINK_LEDS)
    255                else
    256                {      
    257                  HalLedSet ( HAL_LED_1, HAL_LED_MODE_FLASH ); // Flash LED to show failure
    258                }
    259          #endif
    260                break;
    261          
    262              case Match_Desc_rsp:
    263                {
    264                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    265                  if ( pRsp )
    266                  {
    267                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    268                    {
    269                      zclZigbeeDevice_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    270                      zclZigbeeDevice_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    271                      // Take the first endpoint, Can be changed to search through endpoints
    272                      zclZigbeeDevice_DstAddr.endPoint = pRsp->epList[0];
    273                      
    274                      HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    275                    }
    276                    osal_mem_free( pRsp );
    277                  }
    278                }
    279                break;
    280            }
    281          }
