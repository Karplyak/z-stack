   3062            {
   3063              for ( i = 0, attrID = discoverCmd->startAttr; i < discoverRspCmd->numAttr; i++, attrID++ )
   3064              {
   3065                if ( !zclFindNextAttrRec( pInMsg->msg->endPoint, pInMsg->msg->clusterId, &attrID, &attrRec ) )
   3066                  break; // Attribute not supported
   3067                
   3068                discoverRspCmd->attrList[i].attrID = attrRec.attr.attrId;
   3069                discoverRspCmd->attrList[i].dataType = attrRec.attr.dataType;
   3070              }
   3071              
   3072              // Are there more attributes to be discovered?
   3073              if ( zclFindNextAttrRec( pInMsg->msg->endPoint, pInMsg->msg->clusterId, &attrID, &attrRec ) )
   3074                discComplete = FALSE;
