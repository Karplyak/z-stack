   3060          static uint8 zclProcessInDiscCmd( zclIncoming_t *pInMsg )
   3061          {
   3062            zclDiscoverCmd_t *discoverCmd;
   3063            zclDiscoverRspCmd_t *discoverRspCmd;
   3064            uint8 discComplete = TRUE;
   3065            zclAttrRec_t attrRec;
   3066            uint16 attrID;
   3067            uint8 i;
   3068            
   3069            discoverCmd = (zclDiscoverCmd_t *)pInMsg->attrCmd;
   3070            
   3071            // Find out the number of attributes supported within the specified range
   3072            for ( i = 0, attrID = discoverCmd->startAttr; i < discoverCmd->maxAttrIDs; i++, attrID++ )
   3073            {
   3074              if ( !zclFindNextAttrRec( pInMsg->msg->endPoint, pInMsg->msg->clusterId, &attrID, &attrRec ) )
   3075                break;
   3076            }
   3077            
   3078            // Allocate space for the response command
   3079            discoverRspCmd = (zclDiscoverRspCmd_t *)osal_mem_alloc( sizeof (zclDiscoverRspCmd_t) 
   3080                                                                   + sizeof ( zclDiscoverInfo_t ) * i );
   3081            if ( discoverRspCmd == NULL )
   3082              return FALSE; // EMEDDED RETURN
   3083            
   3084            discoverRspCmd->numAttr = i;
   3085            if ( discoverRspCmd->numAttr != 0 )
   3086            {
   3087              for ( i = 0, attrID = discoverCmd->startAttr; i < discoverRspCmd->numAttr; i++, attrID++ )
   3088              {
   3089                if ( !zclFindNextAttrRec( pInMsg->msg->endPoint, pInMsg->msg->clusterId, &attrID, &attrRec ) )
   3090                  break; // Attribute not supported
   3091                
   3092                discoverRspCmd->attrList[i].attrID = attrRec.attr.attrId;
   3093                discoverRspCmd->attrList[i].dataType = attrRec.attr.dataType;
   3094              }
   3095              
   3096              // Are there more attributes to be discovered?
   3097              if ( zclFindNextAttrRec( pInMsg->msg->endPoint, pInMsg->msg->clusterId, &attrID, &attrRec ) )
   3098                discComplete = FALSE;
   3099            }
   3100            
   3101            discoverRspCmd->discComplete = discComplete;
   3102            zcl_SendDiscoverRspCmd( pInMsg->msg->endPoint, &pInMsg->msg->srcAddr, 
   3103                                    pInMsg->msg->clusterId, discoverRspCmd, ZCL_FRAME_SERVER_CLIENT_DIR, 
   3104                                    true, pInMsg->hdr.transSeqNum );
   3105            osal_mem_free( discoverRspCmd );
   3106            
   3107            return TRUE;
   3108          }
